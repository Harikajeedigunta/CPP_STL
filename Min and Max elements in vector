#include<bits/stdc++.h>
using namespace std;
int main()
{
    vector<int>v;
    int n;
    cin>>n;
    v.resize(n);
    for(int i=0;i<n;i++)
    {
        cin>>v[i];
    }
    cout<<endl;
    for(int i=0;i<n;i++)
    {
        cout<<v[i]<<" ";
    }
    cout<<endl;
    if(!v.empty())
    {
        cout<<*min_element(v.begin(),v.end())<<" ";
        cout<<*max_element(v.begin(),v.end());
    }
    else
    {
        cout<<"Vector is empty";
    }
}

Input:
5
3 1 5 23 2

Output:
3 1 5 23 2 
1 23
